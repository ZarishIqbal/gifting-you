{
    "presets": ["module:metro-react-native-babel-preset"],
    "plugins": ["transform-inline-environment-variables",
      [
        "module-resolver",
        {
          "root": ["./src"],
          "extensions": [
            ".ios.ts",
            ".android.ts",
            ".ts",
            ".ios.tsx",
            ".android.tsx",
            ".tsx",
            ".jsx",
            ".js",
            ".json"
          ],
          "alias": {
            "@navigation": "./src/navigation",
            "@components": "./src/components",
            "@assets": "./src/assets",
            "@utils":"./src/utils",
            "@styles":"./src/styles",
            "@config":"./src/config",
            "@constants":"./src/redux/constants",
            "@actions":"./src/redux/actions",
            "@redux":"./src/redux",
            "@screens":"./src/screens",
          }
        }
      ]
    ]
  }